{
  "name": "supravatm/module-mock-data-generator",
  "description": "A Magento 2 module that generates mock data (products, orders, customers) with GraphQL support.",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "authors": [
    {
      "name": "Supravat Mondal",
      "email": "supravat.com@email.com",
      "homepage": "https://github.com/supravat", 
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4 <9.0",
    "magento/framework": "103.*",
    "magento/module-backend": "*",
    "magento/module-ui": "*",
    "magento/module-store": "*",
    "magento/module-catalog": "*",
    "fakerphp/faker": "^1.21"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "SMG\\MockDataGenerator\\": ""
    }
  },
  "extra": {
    "map": [
      [
        "*",
        "SMG/MockDataGenerator"
      ]
    ]
  },
  "keywords": [
    "magento2",
    "graphql",
    "mock-data",
    "orders",
    "products",
    "customers"
  ]
}
