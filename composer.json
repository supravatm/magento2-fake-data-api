{
  "name": "supravatm/magento2-module-fake-data-api",
  "description": "A lightweight and non-intrusive(no DB bloat unless you extend it to save) Magento 2 module to generate fake customers, products, and orders via GraphQL & REST APIs.",
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
        "MIT"
    ],
  "authors": [
    {
      "name": "Supravat Mondal",
      "email": "supravat.com@email.com",
      "homepage": "https://github.com/supravatm", 
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7||^8",
    "magento/framework": "^103",
    "fakerphp/faker": "^1.21"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "SMG\\MockDataGenerator\\": ""
    }
  },
  "extra": {
    "map": [
      [
        "*",
        "SMG/MockDataGenerator"
      ]
    ]
  },
  "keywords": [
    "magento2",
    "graphql",
    "mock-data",
    "test-data",
    "sample-data",
    "fake-data",
    "data-faker",
    "orders",
    "products",
    "customers"
  ]
}
